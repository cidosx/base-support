{
	"name": "ginnerpeace/base-support",
	"homepage": "https://github.com/ginnerpeace/base-support",
	"description": "用composer管理项目公共包的代码结构(demo)",
	"license": "proprietary",
	"authors": [
		{
			"name": "ginnerpeace",
			"email": "ginnerpeace@live.com"
		}
	],
	"require": {
		"php": ">=5.5.9"
	},
	"autoload": {
		"psr-4": {
			"Console\\": "src/Console",
			"Const\\": "src/Consts",
			"Contract\\": "src/Contracts",
			"Enum\\": "src/Enums",
			"Exceptions\\": "src/Exceptions",
			"Helper\\": "src/Helpers",
			"Job\\": "src/Jobs",
			"Lib\\": "src/Libraries",
			"Middleware\\": "src/Middlewares",
			"Providers\\": "src/Providers",
			"Trait\\": "src/Traits"
		},
		"files": [
			"src/consts.php",
			"src/helpers.php"
		]
	}
}
